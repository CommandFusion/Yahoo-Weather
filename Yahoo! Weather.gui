      <cmd name="ClearInputField" target="s1" value="">dummy</cmd>
      <cmd name="ClearList" js="deleteList()" jsSendsCommand="False">dummy</cmd>
      <cmd name="ShowStatusBar" target="d17931" value="1">dummy</cmd>
    </system>
