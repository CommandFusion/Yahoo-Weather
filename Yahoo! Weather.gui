    <imagefolder>Assets</imagefolder>
    <debug loaderrors="0" connectionerrors="0" />
    <devices>
      <device name="G2M iPad Dev">9869B9F35ADA54E672B351984458A594</device>
      <device name="Jordan iPod Touch 1G Dev">6973E7B94A76BB351EB86BDC3313C7B2</device>
